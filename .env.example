# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Application Client ID
CLIENT_ID=your_client_id_here

# Discord Guild/Server ID (untuk development, command register per-guild lebih cepat)
GUILD_ID=your_guild_id_here

# Browser untuk mengambil cookies YouTube (bypass bot detection)
# Opsi: chrome, firefox, edge, brave, opera, vivaldi, none
# Set "none" untuk server/hosting (bot akan coba tanpa cookies dulu)
COOKIES_BROWSER=none
